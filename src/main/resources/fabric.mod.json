{
	"schemaVersion": 1,
	"id": "notcharrowutils",
	"version": "${version}",
	"name": "NotchArrow's Utils",
	"description": "Utilities for Minecraft through chat commands. All of the features of this mod are subcommands under '/utils'",
	"authors": [
		"NotchArrow"
	],
	"contact": {
		"homepage": "https://github.com/NotchArrow/notcharrowutils"
	},
	"license": "MIT",
	"icon": "assets/notcharrowutils/icon.png",
	"environment": "client",
	"entrypoints": {
		"client": [
			"notcharrowutils.NotchArrowUtils"
		],
		"modmenu": [
			"notcharrowutils.config.NotchArrowUtilsModMenu"
		]
	},
	"mixins": [
		"notcharrowutils.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.10",
		"minecraft": "~1.21.4",
		"java": ">=17",
		"fabric-api": "*",
		"modmenu": "*",
		"cloth-config": "*"
	}
}