{
	"schemaVersion": 1,
	"id": "notcharrowutils",
	"version": "${version}",
	"name": "NotchArrow's Utils",
	"description": "Utilities for Minecraft through chat commands. All of the features of this mod are subcommands under '/utils'",
	"authors": [
		"NotchArrow"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/notcharrowutils",
		"issues": "https://github.com/NotchArrow/NotchArrowUtils/issues",
		"sources": "https://github.com/NotchArrow/NotchArrowUtils"
	},
	"custom": {
		"modmenu": {
			"links": {
				"modmenu.discord": "https://discord.com/invite/ZCaGkZeb4C"
			}
		}
	},
	"license": "MIT",
	"icon": "assets/notcharrowutils/icon.png",
	"environment": "client",
	"entrypoints": {
		"client": [
          "com.notcharrow.notcharrowutils.NotchArrowUtils"
		],
		"modmenu": [
          "com.notcharrow.notcharrowutils.config.NotchArrowUtilsModMenu"
		]
	},
	"mixins": [
		"notcharrowutils.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.10",
		"minecraft": "1.21.9",
		"java": ">=17",
		"fabric-api": "*",
		"modmenu": "*",
		"cloth-config": "*"
	}
}